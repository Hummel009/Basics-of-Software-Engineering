package main.java.hummel;

public class Lab1_01 {
	public static void main(String[] args) {
		double x = 0.6;
		for (int n = 10; n <= 15; n++) {
			while(x <= 1.1) {
				double firstPart = Math.exp(Math.log(Math.exp(n*x)/2 + Math.exp(Math.log(n * x) / 3))) / 3;
				double secondPart = 0;
				for (int k = 1; k <= n; k++) {
					secondPart += Math.exp(Math.log(Math.exp(1.2 * k) + (k - 10) / (k + 30)) / k)/ Math.sqrt((k * Math.exp(Math.log(n + 5) / 3))) + Math.log(Math.sqrt(n * x));
				}
				x +=0.1;
			}
		}
	}
}